<templateSet group="Swagger">
  <template name="am" value="@ApiModel(&quot;$var$&quot;)" description="swagger ApiModel" toReformat="false" toShortenFQNames="true">
    <variable name="var" expression="clipboard()" defaultValue="" alwaysStopAt="false" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="amp" value="@ApiModelProperty(value = &quot;$value$&quot;, dataType = &quot;string&quot;)" description="swagger ApiModelProperty" toReformat="true" toShortenFQNames="true">
    <variable name="value" expression="clipboard()" defaultValue="" alwaysStopAt="false" />
    <context>
      <option name="JAVA_DECLARATION" value="true" />
    </context>
  </template>
  <template name="add" value="**&#10; *&#10; * @author : $author$&#10; * @date : $date$ $time$ &#10;$params$&#10;$return$&#10; */" description="add comment" toReformat="false" toShortenFQNames="true">
    <variable name="author" expression="user()" defaultValue="" alwaysStopAt="true" />
    <variable name="date" expression="date()" defaultValue="" alwaysStopAt="true" />
    <variable name="time" expression="time()" defaultValue="" alwaysStopAt="true" />
    <variable name="params" expression="groovyScript(&quot;groovy code&quot;)" defaultValue="" alwaysStopAt="true" />
    <variable name="return" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
</templateSet>